{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@public/*": ["public/*"],
      "@data/*":["src/data/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*":["src/layouts/*"],
      "@styles/*":["src/styles/*"],
      "@utils/*":["src/utils/*"],
      "@interfaces/*":["src/interfaces/*"]
    }
  }
}